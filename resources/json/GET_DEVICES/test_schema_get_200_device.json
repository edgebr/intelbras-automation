{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "serialId": {
        "type": "string"
      },
      "name": {
        "type": ["string", "null"]
      },
      "blacklisted": {
        "type": "boolean"
      },
      "manufacturingDate": {
        "type": ["string", "null"],
        "format": "date-time"
      },
      "productCode": {
        "type": "string"
      },
      "ipAddress": {
        "type": ["string", "null"]
      },
      "firstConnection": {
        "type": ["string", "null"],
        "format": "date-time"
      }
    },
    "required": [
      "id",
      "serialId",
      "blacklisted",
      "productCode"
    ]
  }
} 